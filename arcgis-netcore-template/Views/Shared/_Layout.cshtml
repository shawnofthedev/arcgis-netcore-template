<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8"/>
    <meta name="viewport" content="width=device-width, initial-scale=1.0"/>

    <title>@ViewData["Title"] - Asp.Net Core Template</title>

    <link href="https://s3-us-west-1.amazonaws.com/patterns.esri.com/files/calcite-web/1.2.5/css/calcite-web.min.css" rel="stylesheet" />
    <link href="~/css/site.css" rel="stylesheet" />

</head>
<body>
    <partial name="_NavBar" />
    <partial name="_CookieConsentPartial" />

    <div class="grid-container">
        @RenderBody()
    </div>
    <div class="footer leader-3 link-dark-gray" role="contentinfo">
        <hr />
        <footer class="grid-container">
            <p>&copy; @System.DateTime.Today.Year - Brand Name</p> 
        </footer> 
    </div>

    <script src="https://s3-us-west-1.amazonaws.com/patterns.esri.com/files/calcite-web/1.2.5/js/calcite-web.min.js"></script>
    <script src="~/js/site.js"></script>
</body>
</html>